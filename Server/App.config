<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
	</startup>
	<system.serviceModel>
		<services>
			<service name="Server.EntitetServer">
				<host>
					<baseAddresses>
						<add baseAddress="net.tcp://localhost:8001" />
					</baseAddresses>
				</host>
				<endpoint address="IEntitet" binding="netTcpBinding"
						contract="Common.IEntitet" />
			</service>
		</services>
		<client>
			<endpoint name="Servis"
				address="net.tcp://localhost:8002/IEntitet"
				binding="netTcpBinding"
				contract="Common.IEntitet" />
		</client>
	</system.serviceModel>
</configuration>